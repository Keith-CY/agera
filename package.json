{
  "name": "agera",
  "version": "0.1.0",
  "private": true,
  "author": "Keith CY <keithwhisper@gmail.com> (https://github.com/Keith-CY)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keith-CY/agera"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "npm run dev & npm run cypress:run",
    "format": "prettier -w --ignore-unknown pages/* pages/**/* components/**/* styles/* cypress/**/*{.ts,.json}"
  },
  "dependencies": {
    "date-fns": "2.16.1",
    "next": "10.0.6",
    "next-i18next": "7.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.2",
    "@types/node": "14.14.22",
    "@types/react": "17.0.1",
    "autoprefixer": "10.2.4",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "6.4.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "postcss": "8.2.4",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "14.0.0",
    "postcss-nesting": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "sass": "1.32.6",
    "tailwindcss": "2.0.2",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "npm run format"
  },
  "nyc": {
    "exclude": [
      "pages/api/__coverage__.js"
    ]
  }
}
